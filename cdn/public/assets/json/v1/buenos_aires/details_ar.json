    "trivia": {
        "items": [
            "هي عاصمة الأرجنتين وأكبر مدنها.",
            "يقع على مصب ريو دي لا بلاتا.",
            "تعتبر \"باريس أمريكا الجنوبية\"",
            "المناخ شبه استوائي مع درجات حرارة شديدة تكون نادرة",
            "مترو أنفاق بوينس آيرس هو أقدم نظام مترو أنفاق في نصف الكرة الجنوبي وأقدمها في العالم الناطق بالإسبانية",
            "تشمل أفضل أندية كرة القدم بوكا جونيورز وريفر بليت والمباراة بينهما معروفة باسم Superclásico.",
            "استضافت كأس العالم 1978 FIFA التي فازت بها Argetina بفوزها على هولندا 5-1 في النهائيات.",
            "فاز فيلمان \"القصة الرسمية\" و \"السر في عيونهما\" بجائزة الأوسكار في فئة \"أفضل فيلم أجنبي\"",
            "أنتجت جامعة بوينس آيرس خمسة فائزين بجائزة نوبل",
            "Tango جزء من التراث الثقافي العالمي."
        ]
    }
